{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "content": {
      "type": "string",
      "enum": ["prepared_session"]
    },
    "uuid": {
      "type": "string",
      "format": "uuid"
    },
    "median_power_consumption": {
      "type": "number"
    },
    "max_power_consumption": {
      "type": "number"
    },
    "skewness_power_consumption": {
      "type": "number"
    },
    "mean_absolute_deviation_consumption": {
      "type": "number"
    },
    "tissue_product": {
      "type": "integer",
    },
    "load_and_speed_type": {
      "type": "integer"
    },
    "label": {
      "type": "number",
      "enum": [0.0, 1.0]
    }
  },
  "required": ["content", "uuid", "median_power_consumption", "max_power_consumption", "skewness_power_consumption", "mean_absolute_deviation_consumption", "tissue_product", "load_and_speed_type", "label"],
  "additionalProperties": false
}
