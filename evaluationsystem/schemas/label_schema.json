{
  "$id": "https://example.com/person.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Label",
  "type": "object",
  "required": [
    "uuid",
    "anomalous",
    "sender"
  ],
  "properties": {
    "uuid": {
      "type": "string",
      "format": "uuid"
    },
    "anomalous": {
      "type": "number"
    },
    "sender": {
      "type": "string",
      "enum": ["expert", "classifier"]
    }
  }
}